// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("COCKROACH_URL")
}

model User {
  id BigInt @id @default(sequence())
  email String @unique
  name String
}