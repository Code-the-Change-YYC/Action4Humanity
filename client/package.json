{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@tanstack/react-query": "^4.26.1",
    "@trpc/client": "^10.15.0",
    "@trpc/react-query": "^10.15.0",
    "@trpc/server": "^10.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.14",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.29.2",
    "vitest-mock-extended": "^1.1.0"
  },
  "eslintConfig": {
    "extends": [
      "../package.json",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "@typescript-eslint/no-restricted-imports": [
        "error",
        {
          "paths": [
            {
              "name": "../server",
              "allowTypeImports": true
            },
            {
              "name": "@server",
              "allowTypeImports": true
            },
            {
              "name": "../node_modules"
            }
          ]
        }
      ]
    }
  }
}
